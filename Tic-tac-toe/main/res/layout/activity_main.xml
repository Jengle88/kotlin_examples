<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TableLayout
        android:id="@+id/tableLayout"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0">

        <TableRow
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:layout_marginStart="65dp"
            android:layout_marginTop="100dp">

            <Spinner
                android:id="@+id/spinner11"
                android:layout_width="wrap_content"
                android:layout_height="74dp"
                android:dropDownWidth="50dp"
                android:entries="@array/Sign" />

            <Spinner
                android:id="@+id/spinner12"
                android:layout_width="wrap_content"
                android:layout_height="74dp"
                android:dropDownWidth="50dp"
                android:entries="@array/Sign" />

            <Spinner
                android:id="@+id/spinner13"
                android:layout_width="wrap_content"
                android:layout_height="74dp"
                android:dropDownWidth="50dp"
                android:entries="@array/Sign" />
        </TableRow>


        <TableRow
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:layout_marginStart="65dp">

            <Spinner
                android:id="@+id/spinner21"
                android:layout_width="70dp"
                android:layout_height="74dp"
                android:dropDownWidth="50dp"
                android:entries="@array/Sign" />

            <Spinner
                android:id="@+id/spinner22"
                android:layout_width="70dp"
                android:layout_height="74dp"
                android:dropDownWidth="50dp"
                android:entries="@array/Sign" />

            <Spinner
                android:id="@+id/spinner23"
                android:layout_width="70dp"
                android:layout_height="74dp"
                android:dropDownWidth="50dp"
                android:entries="@array/Sign" />
        </TableRow>


        <TableRow
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:layout_marginStart="65dp">

            <Spinner
                android:id="@+id/spinner31"
                android:layout_width="70dp"
                android:layout_height="74dp"
                android:dropDownWidth="50dp"
                android:entries="@array/Sign" />

            <Spinner
                android:id="@+id/spinner32"
                android:layout_width="70dp"
                android:layout_height="74dp"
                android:dropDownWidth="50dp"
                android:entries="@array/Sign" />

            <Spinner
                android:id="@+id/spinner33"
                android:layout_width="70dp"
                android:layout_height="74dp"
                android:dropDownWidth="50dp"
                android:entries="@array/Sign" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="54dp">

            <TextView
                android:id="@+id/status"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="9dp"
                android:layout_marginStart="-9dp"
                android:layout_marginTop="10dp"
                android:layout_column="2" />

        </TableRow>

    </TableLayout>

</android.support.constraint.ConstraintLayout>


